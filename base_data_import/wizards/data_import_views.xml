<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_data_import" model="ir.ui.view">
        <field name="name">view.data.import</field>
        <field name="model">data.import</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="file_name" invisible="1" />
                            <field
                                name="import_file"
                                widget="binary"
                                filename="file_name"
                            />
                        </group>
                    </group>
                </sheet>
                <footer>
                    <button name="cancel" string="Cancel" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
